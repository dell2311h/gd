#errorExplanation { width: 500px; border:2px solid #FF0000; border-bottom: 2px solid #FF0000; background: #F0F0F0; margin-bottom: 10px;}
#errorExplanation h2 {margin:0; padding:0; background: #CC0000; font-size: 13px; font-family: Arial,  Helvetica, sans-serif; font-weight: bold; color:#FFFFFF; height: 20px; padding-top: 3px; padding-left: 10px; }
#errorExplanation p { padding: 5px; color:#000000;}
#errorExplanation ul { margin: 0; padding:0; margin-top: 7px; margin-bottom: 7px; margin-left: 30px; }
#errorExplanation ul li { list-style-type: square; font-size: 13px; font-family:  Arial, Helvetica, sans-serif; color:#000000;}

.flashmessage {margin:0; padding:0; background: #CC0000; font-size: 13px; font-family:  Arial, tahoma,Helvetica, sans-serif; font-weight: bold; color:#FFFFFF; height: 20px; padding-top: 3px; padding-left: 10px; }
	
