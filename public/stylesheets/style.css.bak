}
section.manager-content-table section.cont ul li.artistsLocCol,
section.manager-content-table header ul li.artistsLocCol{
    width:120px;
	border-left:1px dashed #e1e1e1;
    border-right:1px dashed #e1e1e1;
    text-align:center;
}

section.manager-content-table header ul li.artistsLoc2Col{
	text-align:center;
}
section.manager-content-table section.cont ul li.artistsLoc2Col{
    position:relative;
