section.manager-content-table section.cont ul li.curatorsControlsCol,
section.manager-content-table header ul li.curatorsControlsCol{
    width:80px;
    text-align:center;
}
section.manager-content-table section.cont ul li.curatorsNameCol,
section.manager-content-table header ul li.curatorsNameCol{
    width:304px;
    border-left:1px dashed #e1e1e1;
