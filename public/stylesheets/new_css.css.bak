/*hover*/
section.manager.mainpage .gallery figure div.hover{
    display:none;
    position:absolute;
    top:0px;
    left:-10px;
    border:1px solid #d1d1d1;
    width:315px;
    height:295px;
    padding:8px;
    background-color:#fff;
    border-radius:3px;
    box-shadow:0px 0px 8px rgba(0,0,0,.3);
    z-index: 1000;
    cursor: default;
}

section.manager.mainpage .gallery figure.last-visible div.hover {
    left:-180px;
}

section.manager.mainpage .gallery figure:hover div.hover{
    display:block;
}
section.manager.mainpage .gallery figure div.hover div.box{
    float:left;
    padding:8px;
    width:151px;
}
section.manager.mainpage .gallery figure div.hover div.box.two{
    width:120px;
    padding-left:12px;
}
section.manager.mainpage .gallery figure div.hover div.box .thumbnail{
    width:149px;
}
section.manager.mainpage .gallery figure div.hover div.box h6,
section.manager.mainpage .gallery figure div.hover div.box time{
    font-size:10px;
    color:#5d5d5d;
}
section.manager.mainpage .gallery figure div.hover div.box h6{
    text-transform:uppercase;
    font-weight:normal;
    margin-top:14px;
}
section.manager.mainpage .gallery figure div.hover div.box time+h4{
    margin-top:14px;
}
section.manager.mainpage .gallery figure div.hover div.box h4{
    font-size: 12px;
    color: #009FE1;
    line-height: 15px;
    font-weight:normal;
}
section.manager.mainpage .gallery figure div.hover div.box address{
    font-size:10px;
    line-height:15px;
    font-style:normal;
}
section.manager.mainpage .gallery figure div.hover div.box address span{
    display:block;
}

section.manager.mainpage .gallery figure div.hover div.box a{
    color: #009fe1;
    font-size:10px;
}

section.manager.mainpage .gallery figure div.hover div.box p.big{
    font-size:12px;
    margin:14px 0;
}
section.manager.mainpage .gallery figure div.hover div.box article{
    font-size:10px;
    line-height:15px;
}
