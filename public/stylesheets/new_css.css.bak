ul.paging li a:hover{
    color:#5d5d5d;
    text-decoration:none;
}

ul.paging li a{
    display:inline-block;
}
ul.paging li.arrow a{
    background: transparent url('/images/arrows.png') no-repeat;
    height:12px;
}
ul.paging li.arrow a.back-to-start{
    width:12px;
    background-position:left bottom
}
ul.paging li.arrow a.back.disable,
ul.paging li.arrow a.back-to-start.disable{
    background-position:left top
}
ul.paging li.arrow a.back{
    width:6px;
    background-position:left bottom;
    margin-right:10px;
}

ul.paging li.arrow a.forward-to-end{
